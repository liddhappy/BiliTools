{
  "name": "catlair-bilitools",
  "version": "0.3.20",
  "description": "this is BiliTools",
  "main": "index.ts",
  "scripts": {
    "build": "babel ./src --out-dir ./dist --extensions \".ts\"",
    "build:rollup": "rollup -c rollup.config.js",
    "deploy": "sls deploy --debug",
    "deploy:multi": "node tools/bootstrap.js --scf",
    "start:multi": "node tools/bootstrap.js --start",
    "dev": "ts-node ./src/run.ts",
    "start": "node ./dist/run.js",
    "liveHeart": "node ./dist/runLiveHeart.js",
    "autoclean": "yarn autoclean -F && node tools/pkgclean.js",
    "prebuild": "rm -rf dist/",
    "predeploy:multi": "cp -rf ./tools/ ./package*.json ./.yarnclean ./dist && cd ./dist && yarn install --production && npm run autoclean && rm -rf tools package*.json .yarnclean &&  cd ../",
    "postbuild": "cd ./dist && rm -rf ./dto/ ./interface/ *.d.js",
    "lint:eslint": "eslint --max-warnings 0  \"**/*.{js,ts}\" --fix",
    "lint:prettier": "prettier --write  \"**/*.{js,json,ts,md}\""
  },
  "author": "catlair",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/crypto-js": "^4.1.1",
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.21",
    "@types/nodemailer": "^6.4.4",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "rollup-plugin-sizes": "^1.0.4",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "crypto-js": "^4.1.1",
    "got": "^11.8.3",
    "lodash": "^4.17.21",
    "nodemailer": "^6.7.2",
    "pako": "1.0.11",
    "qs": "^6.10.3",
    "tencentcloud-sdk-nodejs": "^4.0.297",
    "tough-cookie": "^4.0.0",
    "tunnel": "^0.0.6"
  }
}
